{
  "name": "@stlucky/cs-frontend",
  "version": "0.1.0",
  "description": "Computer Science in Frontend",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "npm run clean && tsc",
    "clean": "del-cli ./lib/*"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "29.1.2",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "del-cli": "5.0.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "jest": "29.1.2",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "./src/**/*.ts"
    ]
  }
}